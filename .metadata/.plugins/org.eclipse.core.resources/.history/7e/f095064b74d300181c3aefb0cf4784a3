package practice;

public class Main {
	public static void main(String[] args) {
		System.out.println("まつなが");
		System.out.println(23 + 49);
		System.out.println(46 * 31);
		System.out.println(-43 + 38);
		int x;
		x = 4;
		System.out.println(x * 50);
		int age;
		age = 20;
		System.out.println("私は"+age+"歳");
	}
}
